{
  "name": "@web-auto-notify/webpack",
  "type": "module",
  "version": "1.0.1",
  "description": "Webpack plugin for detect web page updates and notify.",
  "author": "",
  "license": "",
  "homepage": "https://gitee.com/bluelightsky/web-auto-notify-plugin",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/bluelightsky/web-auto-notify-plugin",
    "directory": "packages/vite-plugin"
  },
  "bugs": {
    "url": "https://gitee.com/bluelightsky/web-auto-notify-plugin/issues"
  },
  "keywords": [
    "webpack",
    "webpack-plugin"
